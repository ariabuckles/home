# This is Git's per-user configuration file.
[user]
    name = Aria Buckles
    email = aria@seesaw.me
[alias]
    p = pull --rebase
    co = checkout
    su = branch --set-upstream-to master
    p = pull --rebase
    br = branch
    lp = log -p
    st = status
    ri = rebase -i
    rc = rebase --continue
    ci = commit
    alias = config --get-regexp ^alias
[core]
    excludesfile = ~/.gitignore
    hooksPath = ~/.githooks
[merge]
    tool = vimdiff
    conflictstyle = diff3
[mergetool]
    prompt = false
